<h1>Student Form</h1>
<%= form_tag new_student_path do %>
  <label>Student name:</label><br>
  <%= text_field_tag :first_name %><br>
  <%= text_field_tag :last_name %><br>

  <label>Student Description</label><br>
  <%= text_area_tag :description %><br>

  <%= submit_tag "Submit New Student" %>
<% end %>
